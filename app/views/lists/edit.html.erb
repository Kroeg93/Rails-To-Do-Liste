<h1>To-Do Liste ändern</h1>

<%= form_for @list do |f| %>
<p>
  <strong><%= f.label :title %></strong><br>
  <%= f.text_field :title %>
</p>
<p>
  <strong><%= f.label :description %></strong><br>
  <%= f.text_area :description %>
</p>
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Zurück', lists_path %>